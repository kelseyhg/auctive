<%if (alerts.success){%>
<% alerts.success.forEach(function(msg){%>
<div class='row'>

	<div class='card bg-warning flash center'>
		<h4> <%= msg %> </h4>
	</div>

</div>

<%});%>

<%}%>

<%if (alerts.error) {%>
<% alerts.error.forEach(function(msg){%>
<div class='row'>

	<div class='card bg-warning flash center'>
		<%= msg %>
	</div>

</div>

<%});%>
<%}%>
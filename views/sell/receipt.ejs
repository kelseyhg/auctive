<div class='container'>
  
  <br> 
  
  <h3>Attendee Check Out</h3>

  <br>

  <% event.attendees.forEach(function(a){%>
  <div class='card list-card shadow-sm'>

    <div class='card-body'>
      <h4 class='card-title'>
        <a href='/sell/show/<%=a.id%>'>
          <%=a.bidNumber%>: <%= a.name %>
        </a>
      </h4>
      <a href='/sell/show/<%=a.id%>' class='btn btn-warning'>check out</a>
    </div>
  </div>
  <%});%>

  <br><br>
  <footer>
    <a href='/event/<%=currentEvent.name%>' class='btn btn-warning'>event page</a>
    <a href='/item/<%=currentEvent.id%>' class='btn btn-warning'>items</a>
    <a href='/attendee/<%=currentEvent.id%>' class='btn btn-warning'>attendees</a>
    <a href='/sell/receipt/<%=currentEvent.id%>' class='btn btn-warning'>checkout</a>
</footer>

</div>